{
    "manifest_version": 2,
    "name": "OBS Data Stream",
    "version": "1.0",

    "description": "Plugin for sending information about currently playing content to a server",

    "icons": {

    },

    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["main.js"]
        }
    ],

    "browser_specific_settings": {
        "gecko": {
            "id": "obs@nbti.net"
        }
    },

    "permissions": [
        "*://nbti.net:3621/*",
        "http://nbti.net:3621/*",
        "activeTab",
        "webRequest"
    ]
}